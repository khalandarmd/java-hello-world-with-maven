pipeline {
	agent { label 'maven2' }
		stages {
                stage('git_checkout') {
		   steps {
                   git branch: 'main', credentialsId: 'Lnt_git_credential', url: 'https://github.com/vidya-1995/java-hello-world-with-maven.git'
			}
		}
      stage('Build') {
		   steps {
             sh "mvn clean install"
			}
		}
  }
}
